<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>boot-echo</string>
    <key>ProgramArguments</key>
    <array>
      <string>##echo_startup_shell_path##</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>##echo_log_stdout##</string>
    <key>StandardErrorPath</key>
    <string>##echo_log_stderr##</string>
  </dict>
</plist>
